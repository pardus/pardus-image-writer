#: ui/MainWindow.glade:629
msgid "grub not found"
msgstr "grub não encontrado"

#: ui/MainWindow.glade:644 ui/MainWindow.glade:867
msgid "Cancel"
msgstr "Cancelar"

#: ui/MainWindow.glade:657
msgid "Install"
msgstr "Instalar"

#: ui/MainWindow.glade:741
msgid "Message "
msgstr "Mensagem"

#: ui/MainWindow.glade:787
msgid "Check ISO online (Pardus only)"
msgstr "Verifique a ISO online (apenas Pardus)"

#: ui/MainWindow.glade:847
msgid "Start"
msgstr "Iniciar"

#: ui/MainWindow.glade:961
msgid "Writing process is finished."
msgstr "O processo de gravação está concluído."

#: ui/MainWindow.glade:987
msgid "You can eject the USB disk."
msgstr "Pode ejetar o disco USB."

#: ui/MainWindow.glade:997
msgid "Exit"
msgstr "Sair"

#: ui/MainWindow.glade:1015
msgid "Write a new file."
msgstr "Gravar um novo ficheiro."
